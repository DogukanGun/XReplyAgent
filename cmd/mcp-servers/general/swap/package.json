    "@types/bun": "^1.2.12",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "husky": "^9.1.7",
    "prettier": "3.2.4"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "2.0.0",
    "@kanalabs/aggregator": "0.7.14-beta.14",
    "@solana/web3.js": "1.98.2",
    "dotenv": "16.5.0",
    "ethers": "6.13.5",
    "@modelcontextprotocol/sdk": "^1.11.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
