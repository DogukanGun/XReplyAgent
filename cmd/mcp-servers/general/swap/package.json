  "scripts": {
    "start": "node dist/index.js",
    "start:sse": "npm run start --sse",
    "dev:sse": "bun run --watch src/index.ts --sse",
    "dev": "bun run --watch src/index.ts",
    "build": "bun build src/*.ts --outdir dist --target node --format cjs",
